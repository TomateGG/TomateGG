local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "üî•@tomatecomidoüî´",
   LoadingTitle = "üî´ Loading Scripts üí•",
   LoadingSubtitle = "Customized by User",
   ConfigurationSaving = {
      Enabled = false,
      FolderName = nil,
      FileName = "CustomHub"
   },
   Discord = {
      Enabled = true,
      Invite = "BpxeNssu", -- Nuevo c√≥digo de Discord
      RememberJoins = true
   },
   KeySystem = false -- No se necesita el sistema de claves en este caso
})

-- Tab: Murder vs Sheriffs Duels
local MurderTab = Window:CreateTab("Murder vs Sheriffs Duels üó°Ô∏èüî´", nil)
MurderTab:CreateButton({
   Name = "RubyHub",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/Deni210/murdersvssherrifsduels/main/rubyhub", true))()
   end,
})
MurderTab:CreateButton({
   Name = "Hitbox Expander",
   Callback = function()
      _G.HeadSize = 10
      _G.Disabled = true

      game:GetService('RunService').RenderStepped:connect(function()
         if _G.Disabled then
            for i, v in next, game:GetService('Players'):GetPlayers() do
               if v.Name ~= game:GetService('Players').LocalPlayer.Name then
                  pcall(function()
                     v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize, _G.HeadSize, _G.HeadSize)
                     v.Character.HumanoidRootPart.Transparency = 0.7
                     v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Really blue")
                     v.Character.HumanoidRootPart.Material = "Neon"
                     v.Character.HumanoidRootPart.CanCollide = false
                  end)
               end
            end
         end
      end)
   end,
})

